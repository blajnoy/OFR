{
  "name": "office_file_redactor",
  "description": "office file redactor",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "office file redactor",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["<all_urls>", "tabs", "storage", "webRequest", "webRequestBlocking"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
